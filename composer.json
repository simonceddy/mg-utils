{
    "name": "simoneddy/crawlers",
    "description": "Utils for crawling and handling data from sites like ModularGrid",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Eddy",
            "email": "simon@simoneddy.com.au"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
       "bin-dir": "bin"
    },
    "autoload": {
       "psr-4": {
          "Eddy\\Crawlers\\": "src"
       }
    },
    "require": {
        "symfony/dom-crawler": "^6.3",
        "guzzlehttp/guzzle": "^7.8",
        "symfony/css-selector": "^6.3",
        "kevinrob/guzzle-cache-middleware": "^4.1",
        "doctrine/cache": "^2.2",
        "illuminate/cache": "^10.24",
        "simoneddy/robots-helper": "dev-master",
        "illuminate/filesystem": "^10.28",
        "league/flysystem": "^3.18",
        "php-ds/php-ds": "^1.4"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/simonceddy/robots-helper"
        }
    ]
}
